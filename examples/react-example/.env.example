# Hydropulse React Example Environment Variables

# Required
REACT_APP_HYDROPULSE_SERVICE_NAME=react-hydropulse-example
REACT_APP_HYDROPULSE_SERVICE_VERSION=1.0.0

# Provider Configuration
REACT_APP_HYDROPULSE_PROVIDER=auto
REACT_APP_HYDROPULSE_ENVIRONMENT=development

# Grafana Configuration
REACT_APP_HYDROPULSE_GRAFANA_URL=http://localhost:3000/api/traces
REACT_APP_HYDROPULSE_GRAFANA_API_KEY=demo-key
REACT_APP_HYDROPULSE_GRAFANA_APP_KEY=react-example

# OpenTelemetry Configuration
REACT_APP_HYDROPULSE_OTEL_ENDPOINT=http://localhost:4318
REACT_APP_HYDROPULSE_OTEL_TOKEN=demo-token

# Optional Configuration
REACT_APP_HYDROPULSE_DEBUG=true
REACT_APP_HYDROPULSE_SAMPLING_RATE=1.0
REACT_APP_HYDROPULSE_BATCH_SIZE=50
REACT_APP_HYDROPULSE_BATCH_DELAY=3000
