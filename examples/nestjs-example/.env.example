# Hydropulse NestJS Example Environment Variables

# Required
HYDROPULSE_SERVICE_NAME=nestjs-hydropulse-example
HYDROPULSE_SERVICE_VERSION=1.0.0

# Provider Configuration
HYDROPULSE_PROVIDER=auto
HYDROPULSE_ENVIRONMENT=development

# Grafana Configuration
HYDROPULSE_GRAFANA_URL=http://localhost:3000/api/traces
HYDROPULSE_GRAFANA_API_KEY=demo-key
HYDROPULSE_GRAFANA_APP_KEY=nestjs-example

# OpenTelemetry Configuration
HYDROPULSE_OTEL_ENDPOINT=http://localhost:4318
HYDROPULSE_OTEL_TOKEN=demo-token

# Optional Configuration
HYDROPULSE_DEBUG=true
HYDROPULSE_SAMPLING_RATE=1.0
HYDROPULSE_BATCH_SIZE=100
HYDROPULSE_BATCH_DELAY=5000
HYDROPULSE_CIRCUIT_BREAKER_THRESHOLD=3
HYDROPULSE_CIRCUIT_BREAKER_TIMEOUT=30000
HYDROPULSE_SANITIZE_PII=true
HYDROPULSE_RETRY_MAX_ATTEMPTS=3
HYDROPULSE_RETRY_INITIAL_DELAY=1000
